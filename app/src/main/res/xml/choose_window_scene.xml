<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/linearLayout3"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toEndOf="@+id/linearLayout"
            android:layout_width="160dp"
            android:layout_height="160dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="20dp"
            android:layout_marginEnd="70dp"
            android:layout_marginStart="75dp"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            motion:layout_editor_absoluteY="49dp"
            android:layout_height="22dp"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="60dp"
            motion:layout_constraintStart_toEndOf="@+id/textView6"
            motion:layout_constraintStart_toStartOf="@+id/textView6"
            android:layout_marginStart="60dp"
            android:id="@+id/textView7" />
        <Constraint
            motion:layout_editor_absoluteY="49dp"
            motion:layout_editor_absoluteX="1dp"
            android:id="@+id/textView" />
        <Constraint
            motion:layout_editor_absoluteY="49dp"
            android:layout_height="22dp"
            android:layout_width="56dp"
            motion:layout_constraintEnd_toStartOf="@+id/textView4"
            android:id="@+id/textView3" />
        <Constraint
            motion:layout_editor_absoluteY="49dp"
            motion:layout_editor_absoluteX="60dp"
            android:id="@+id/textView2" />
        <Constraint
            motion:layout_editor_absoluteY="49dp"
            android:layout_height="22dp"
            android:layout_width="60dp"
            motion:layout_constraintStart_toEndOf="@+id/textView4"
            android:id="@+id/textView5" />
        <Constraint
            android:layout_height="22dp"
            android:layout_width="56dp"
            android:id="@+id/textView4"
            motion:layout_constraintBottom_toTopOf="@+id/calendarRecyclerView"
            motion:layout_constraintStart_toStartOf="@+id/textView3"
            android:layout_marginStart="56dp" />
        <Constraint
            motion:layout_editor_absoluteY="49dp"
            motion:layout_editor_absoluteX="291dp"
            android:id="@+id/textView6" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="230dp"
            android:id="@+id/monthYearTV2"
            motion:layout_constraintStart_toEndOf="@+id/button"
            motion:layout_constraintEnd_toStartOf="@+id/button2"
            motion:layout_constraintHorizontal_bias="0.5" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>